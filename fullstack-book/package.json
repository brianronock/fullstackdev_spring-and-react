{
  "name": "fullstack-book",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "cross-env LOCAL_DOCS=external docusaurus start",
    "build:ci": "docusaurus build",
    "build": "docusaurus build",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "docs:javadoc": "mvn -q -f ../Part1_RESTful-API-SpringBoot/pom.xml -DskipTests javadoc:javadoc && rm -rf static/javadoc && mkdir -p static/javadoc && rsync -a ../Part1_RESTful-API-SpringBoot/target/site/apidocs/ static/javadoc/",
    "start:with-javadoc": "npm run docs:javadoc && cross-env LOCAL_DOCS=external docusaurus start"
  },
  "dependencies": {
    "@docusaurus/core": "3.8.1",
    "@docusaurus/preset-classic": "3.8.1",
    "@mdx-js/react": "^3.0.0",
    "clsx": "^2.0.0",
    "prism-react-renderer": "^2.3.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "3.8.1",
    "@docusaurus/types": "3.8.1",
    "cross-env": "^10.0.0"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 3 chrome version",
      "last 3 firefox version",
      "last 5 safari version"
    ]
